// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Produto {
  id    Int    @id @default(autoincrement())
  nome  String
  preco Float
}

model Categoria {
  id        Int    @id @default(autoincrement())
  nome      String
  descricao String
}
